@{
    ViewBag.Title = "SelectNavigation";
}
@model List<Shop.Model.Menu>
<div class="pageContent">
    <div class="pageFormContent" layouth="58">
        <ul class='tree  expand'>
            @foreach (var m in Model)
            {
                <li><a href="javascript:;" onclick="$.bringBack({Id:'@m.Id', MenuName:'@m.Menu_Name'});">@m.Menu_Name</a></li>
            }
        </ul>
    </div>
</div>