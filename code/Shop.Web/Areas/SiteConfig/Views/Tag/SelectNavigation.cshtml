@{
    var navigationList = ViewBag.NavigationList as List<Shop.Model.Navigation>;
}
<div class="pageContent">
    <div class="pageFormContent" layouth="58">
        <ul class='tree  expand'>
            @foreach (var m in navigationList)
            {
                <li><a href="javascript:;" onclick="$.bringBack({Id:'@m.Id', NavigationName:'@m.Navigation_Name'});">@m.Navigation_Name</a></li>
            }
        </ul>
        @Html.Raw(ViewBag.NavigationHtml)
    </div>
</div>